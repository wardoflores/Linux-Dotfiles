planets=true
galaxies=false
diagrams=true
cloudMaps=true
orbits=true
celestialSphere=true
nightMaps=true
atmospheres=true
smoothLines=true
eclipseShadows=true
ringShadows=true
boundaries=false
autoMag=true
cometTails=true
markers=false
partialTrajectories=false
nebulae=true
openClusters=false
globulars=false
gridGalactic=false
gridEcliptic=false
gridHorizontal=false
